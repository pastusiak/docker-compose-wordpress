{
    "name": "pastusiak/website-skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A skeleton to start a new Wordpress website",
    "require": {
        "php": "^7.2.0",
        "johnpbloch/wordpress": "5.3.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "wordpress/"
        }
    }
}
